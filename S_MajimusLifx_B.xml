<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upn-org:service-1-0">
    <specVersion>
        <major>1</major>
        <minor>0</minor>
    </specVersion>
	<serviceStateTable>
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>ApiKey</name>
			<dataType>string</dataType>
			<defaultValue>xxxx</defaultValue>
			<shortCode>apikey</shortCode>
		</stateVariable>
		<stateVariable>
			<sendEventsAttribute>no</sendEventsAttribute>
			<name>LightId</name>
			<dataType>string</dataType>
			<defaultValue>xxxx</defaultValue>
			<shortCode>lightid</shortCode>
		</stateVariable>
	</serviceStateTable>
	<actionList>
		<action>
			<name>SetApiKey</name>
				<argumentList>
					<argument>
						<name>newApiKeyValue</name>
						<direction>in</direction>
						<relatedStateVariable>ApiKey</relatedStateVariable>
					</argument>
			</argumentList>
		</action>
		<action>
			<name>SetLightId</name>
				<argumentList>
					<argument>
						<name>newLightIdValue</name>
						<direction>in</direction>
						<relatedStateVariable>LightId</relatedStateVariable>
					</argument>
			</argumentList>
		</action>
	</actionList>
</scpd>